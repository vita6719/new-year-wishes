import random

# –°–ø–∏—Å–æ–∫ –ø–æ–∂–µ–ª–∞–Ω–∏–π
greetings = [
    "–ñ–µ–ª–∞—é –≤ –Ω–æ–≤–æ–º –≥–æ–¥—É –º–Ω–æ–≥–æ —Ä–∞–¥–æ—Å—Ç–∏, —Å—á–∞—Å—Ç—å—è –∏ —É–¥–∞—á–∏!",
    "–ü—É—Å—Ç—å —Ç–≤–æ–∏ –º–µ—á—Ç—ã —Å–±—ã–≤–∞—é—Ç—Å—è, –∫–∞–∫ —ë–ª–æ—á–Ω—ã–µ –∏–≥—Ä—É—à–∫–∏, –≤–∏—Å—è—Ç –Ω–∞ —ë–ª–∫–µ!",
    "–ü—É—Å—Ç—å –ù–æ–≤—ã–π –≥–æ–¥ –ø—Ä–∏–Ω–µ—Å–µ—Ç –º–æ—Ä–µ –ø–æ–∑–∏—Ç–∏–≤–∞ –∏ –æ–∫–µ–∞–Ω –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç–µ–π!",
    "–ñ–µ–ª–∞—é –∑–¥–æ—Ä–æ–≤—å—è, –∫–∞–∫ —É —Å–µ–≤–µ—Ä–Ω–æ–≥–æ –æ–ª–µ–Ω—è, –∏ —ç–Ω–µ—Ä–≥–∏–∏, –∫–∞–∫ —É –Ω–æ–≤–æ–≥–æ–¥–Ω–µ–π –≥–∏—Ä–ª—è–Ω–¥—ã!",
    "–ü—É—Å—Ç—å –≤—Å–µ —Ç–≤–æ–∏ —Å—Ç–∞—Ä—Ç–∞–ø—ã –≤–∑–ª–µ—Ç—è—Ç —Ç–∞–∫ –∂–µ –≤—ã—Å–æ–∫–æ, –∫–∞–∫ –±–µ–Ω–≥–∞–ª—å—Å–∫–∏–µ –æ–≥–Ω–∏!",
    "–í –Ω–æ–≤–æ–º –≥–æ–¥—É –±—É–¥—å —Ç–∞–∫–∏–º –∂–µ —è—Ä–∫–∏–º –∏ –Ω–µ–ø—Ä–µ–¥—Å–∫–∞–∑—É–µ–º—ã–º, –∫–∞–∫ –ø—Ä–∞–∑–¥–Ω–∏—á–Ω—ã–π —Ñ–µ–π–µ—Ä–≤–µ—Ä–∫!",
    "–ñ–µ–ª–∞—é, —á—Ç–æ–±—ã –Ω–∞ —Ç–≤–æ–µ–º –ø—É—Ç–∏ –≤—Å–µ–≥–¥–∞ –≥–æ—Ä–µ–ª –∑–µ–ª–µ–Ω—ã–π —Å–≤–µ—Ç –∫ —Å—á–∞—Å—Ç—å—é!",
    "–ü—É—Å—Ç—å –≥–æ–¥ –ø—Ä–æ–π–¥–µ—Ç –ª–µ–≥–∫–æ, –∫–∞–∫ –Ω–æ–≤–æ–≥–æ–¥–Ω–∏–π —Ç–æ—Å—Ç!",
]

# –°–ø–∏—Å–æ–∫ –∑–∞–±–∞–≤–Ω—ã—Ö –ø—Ä–µ–¥—Å–∫–∞–∑–∞–Ω–∏–π
funny_predictions = [
    "–í —Å–ª–µ–¥—É—é—â–µ–º –≥–æ–¥—É —Ç—ã —Å—Ç–∞–Ω–µ—à—å –≥—É—Ä—É Python!",
    "–¢–µ–±—è –∂–¥–µ—Ç –Ω–µ–∑–∞–±—ã–≤–∞–µ–º–∞—è –≤—Å—Ç—Ä–µ—á–∞ —Å –∑–∞–≥–∞–¥–æ—á–Ω—ã–º –±–∞–≥–æ–º, –Ω–æ —Ç—ã –µ–≥–æ –ø–æ–±–µ–¥–∏—à—å!",
    "–ö–æ–¥, –∫–æ—Ç–æ—Ä—ã–π —Ç—ã –Ω–∞–ø–∏—à–µ—à—å, —Å–ø–∞—Å—ë—Ç –º–∏—Ä! –ù—É –∏–ª–∏ —Ö–æ—Ç—è –±—ã —É—Ç—Ä–µ–Ω–Ω–∏–π –∫–æ—Ñ–µ.",
    "–¢–≤–æ–π –∫–æ—Ç –∏–ª–∏ —Å–æ–±–∞–∫–∞ –Ω–∞—É—á–∞—Ç—Å—è –∫–æ–¥–∏—Ç—å –≤–º–µ—Å—Ç–µ —Å —Ç–æ–±–æ–π!",
    "–¢—ã —Å–ª—É—á–∞–π–Ω–æ –ø—Ä–∏–¥—É–º–∞–µ—à—å –∞–ª–≥–æ—Ä–∏—Ç–º, –∫–æ—Ç–æ—Ä—ã–π —Ä–µ—à–∏—Ç –≤—Å–µ –ø—Ä–æ–±–ª–µ–º—ã —á–µ–ª–æ–≤–µ—á–µ—Å—Ç–≤–∞.",
    "–¢–≤–æ—è –ª—é–±–∏–º–∞—è –ø—Ä–æ–≥—Ä–∞–º–º–∞ –Ω–∞–∫–æ–Ω–µ—Ü-—Ç–æ –∑–∞—Ä–∞–±–æ—Ç–∞–µ—Ç —Å –ø–µ—Ä–≤–æ–≥–æ —Ä–∞–∑–∞.",
]

def generate_wish():
    """–ì–µ–Ω–µ—Ä–∞—Ü–∏—è –ø–æ–∂–µ–ª–∞–Ω–∏—è –∏ –ø—Ä–µ–¥—Å–∫–∞–∑–∞–Ω–∏—è."""
    wish = random.choice(greetings)
    prediction = random.choice(funny_predictions)
    return f"{wish}\n\n–ê –≤–æ—Ç –∏ –ø—Ä–µ–¥—Å–∫–∞–∑–∞–Ω–∏–µ: {prediction}"

if __name__ == "__main__":
    print("üéÑ –î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å –≤ –≥–µ–Ω–µ—Ä–∞—Ç–æ—Ä –Ω–æ–≤–æ–≥–æ–¥–Ω–∏—Ö –ø–æ–∂–µ–ª–∞–Ω–∏–π! üéÑ")
    print("-" * 50)
    print(generate_wish())
    print("-" * 50)
    input("–ù–∞–∂–º–∏—Ç–µ Enter, —á—Ç–æ–±—ã –ø–æ–ª—É—á–∏—Ç—å –µ—â—ë –æ–¥–Ω–æ –ø–æ–∂–µ–ª–∞–Ω–∏–µ –∏–ª–∏ –∑–∞–∫—Ä—ã—Ç—å –ø—Ä–æ–≥—Ä–∞–º–º—É.")
